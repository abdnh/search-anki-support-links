{
    "manifest_version": 3,
    "name": "Search Anki Support Links",
    "description": "Search Anki Support Links",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_icon": "anki.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "anki.png",
        "128": "anki.png"
    },

    "permissions": ["tabs"]
}
